#After cloning this repo ensure to rename this file to application.properties

spring.application.name=employeeManagementBackend
server.port=8081
#MySQl Configurations
spring.datasource.url=jdbc:mysql://localhost:3306/<db_name>
spring.datasource.username=<enter your database username>
spring.datasource.password=<enter your database password>


#Hibernate JPA settings
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true